<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= PageTitle ? PageTitle : 'Apostille Documents'  %></title>

    <% const desc = (typeof metaDescription !== 'undefined') ? metaDescription : 'Fast apostille services for FBI and other official documents. Secure upload, tracked shipping and expedited processing.' %>
    <% const keys = (typeof metaKeywords !== 'undefined') ? metaKeywords : 'apostille, FBI apostille, apostille online, document authentication' %>

    <meta name="description" content="<%= desc %>">
    <meta name="keywords" content="<%= keys %>">
    <meta name="robots" content="index, follow">

    <!-- Open Graph -->
    <meta property="og:title" content="<%= PageTitle ? PageTitle : 'Apostille Documents' %>">
    <meta property="og:description" content="<%= desc %>">
    <% if (typeof path !== 'undefined') { %>
    <meta property="og:url" content="<%= path %>">
    <% } %>
    <meta property="og:type" content="website">
    <% const metaImg = (typeof metaImage !== 'undefined') ? metaImage : '/docs/img/cover.jpg' %>
    <meta property="og:image" content="<%= metaImg %>">

    <!-- Twitter -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= PageTitle ? PageTitle : 'Apostille Documents' %>">
    <meta name="twitter:description" content="<%= desc %>">

    <link rel="stylesheet" href="/css/all.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/style.css">
   <meta name="google-site-verification" content="oBrP0GqvcUKsicXa24HbWIfMbgePVmMCfuOYztFHTPc" />
  <link rel="icon" href="img/icon.ico" type="image/x-icon">
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);} gtag('js', new Date()); gtag('config', 'AW-16683487822');
    </script>

    <link rel="canonical" href="<%= canonicalUrl %>" />

</head>

